<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="DataAccess" namespace="DataAccess.Model">

  <class name="Objednavka" table="objednavka">
    <id name="Id" column="id">
      <generator class="native" />
    </id>
    <property name="Oznaceni"  column="oznaceni"/>
    <property name="Mnozstvi"  column="mnozstvi"/>
    <property name="Cena"  column="cena"/>

    <many-to-one name="User" column="user_id" foreign-key="user_id" />

  </class>

</hibernate-mapping>