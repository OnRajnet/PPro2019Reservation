
@using DataAccess.Model
@model IList<Objednavka>


@{
    ViewBag.Title = "Index";
}


<h2>Objednávky</h2>

<table class="table table-bordered table-hover table-condensed">
    <thead>
        <tr>
            <th>ID</th>
            <th>Označení</th>
            <th>Množství</th>
            <th>Cena</th>
            <th>Uživatel</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Objednavka o in Model)
        {
            <tr>
                <td>@o.Id</td>
                <td>@o.Oznaceni</td>
                <td>@o.Mnozstvi</td>
                <td>@o.Cena</td>
                <td>@o.User.Login</td>
            </tr>
        }
    </tbody>
</table>



