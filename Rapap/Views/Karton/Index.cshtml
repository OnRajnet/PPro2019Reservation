
@using DataAccess.Model
@model List<Kartonaz>


@{
    ViewBag.Title = "Kartonáž";
}

<h2 style="margin-top:0;">Dostupná kartonáž</h2>
<br />
<div style="overflow-x:auto">
    <table class="table table-hover table-condensed">
        <thead>
            <tr>
                <th>ID</th>
                <th>Název</th>
                <th>Rozměr</th>
                <th>Počet kusů</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Kartonaz k in Model)
            {
                <tr>
                    <td>@k.Id</td>
                    <td>@k.Oznaceni</td>
                    <td>@k.Rozmer</td>
                    <td>@k.Pocet</td>
                </tr>
            }
        </tbody>
    </table>
</div>